{
  "name": "captain_carts",
  "version": "1.0.0",
  "description": "Captain Carts",
  "main": "assets/main.scss",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "all": "0.0.0",
    "autoprefixer": "^9.7.4",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "braces": "^3.0.2",
    "bulma": "^0.7.5",
    "bulma-carousel": "^4.0.4",
    "jquery": "^3.4.1",
    "node-sass": "^4.13.1",
    "postcss-cli": "^6.1.3",
    "tar": "^4.4.18"
  },
  "devDependencies": {},
  "scripts": {
    "jekyll-build": "bundle exec jekyll build --config _config.yml,_config_dev.yml",
    "css-build": "node-sass --omit-source-map-url assets/main.scss assets/css/main.css",
    "css-postcss": "postcss --use autoprefixer --output css/main.css css/main.css",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byronroark/captain_carts.git"
  },
  "author": "Byron Roark",
  "license": "ISC",
  "homepage": "https://captaincarts.com"
}
